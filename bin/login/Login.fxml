<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginController">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children>
            <ImageView fitHeight="609.0" fitWidth="808.0" layoutX="-1.0" layoutY="-3.0">
               <image>
                  <Image url="@../../img/login.png" />
               </image>
            </ImageView>
            <TextField fx:id="id" layoutX="215.0" layoutY="146.0" prefHeight="36.0" prefWidth="218.0" style="-fx-background-color: transparent;">
               <font>
                  <Font size="20.0" />
               </font></TextField>
            <TextField fx:id="password" layoutX="218.0" layoutY="226.0" prefHeight="36.0" prefWidth="208.0" style="-fx-background-color: transparent;">
               <font>
                  <Font size="20.0" />
               </font></TextField>
            <Button fx:id="login" layoutX="63.0" layoutY="297.0" mnemonicParsing="false" onAction="#loginCK" opacity="0.0" prefHeight="47.0" prefWidth="145.0" text="登录" />
            <Button fx:id="register" layoutX="269.0" layoutY="297.0" mnemonicParsing="false" onAction="#resCK" opacity="0.0" prefHeight="47.0" prefWidth="145.0" text="注册" />
         </children>
      </Pane>
   </children>
</StackPane>
